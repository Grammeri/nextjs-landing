generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Entitlement {
  id                String   @id @default(cuid())
  email             String
  product           String
  provider          String
  checkoutSessionId String   @unique
  access            Boolean  @default(true)
  createdAt         DateTime @default(now())

  @@index([email])
}
